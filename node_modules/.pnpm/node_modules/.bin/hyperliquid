#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/hyperliquid/dist/bin/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/hyperliquid/dist/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/hyperliquid/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/hyperliquid/dist/bin/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/hyperliquid/dist/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/hyperliquid/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules/@nktkas/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/@nktkas+hyperliquid@0.26.0_typescript@5.9.3/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nktkas/hyperliquid/dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../@nktkas/hyperliquid/dist/bin/cli.js" "$@"
fi
