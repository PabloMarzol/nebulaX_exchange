#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/dist/cli/bin/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/dist/cli/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/dist/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/dist/cli/bin/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/dist/cli/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/dist/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules/porto/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/porto@0.2.35_@tanstack+react-query@5.90.10_react@18.3.1__@types+react@18.3.27_@wagmi+co_c3f5da5ba4323f55c4c8df1168c9c02f/node_modules:/home/user/nebulaX_exchange/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../porto/dist/cli/bin/index.js" "$@"
else
  exec node  "$basedir/../porto/dist/cli/bin/index.js" "$@"
fi
